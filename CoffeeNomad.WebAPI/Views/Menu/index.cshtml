<body>
    <div id="active-block">
        <header class="property-dashboard" role="banner">
            <div class="frame">
                <div class="logo">
                    <div class="div">
                        <div class="coffee">&nbsp;&nbsp; Coffee</div>
                        <div class="text-wrapper">NOMAD</div>
                    </div>
                </div>
                <nav class="navbar" role="navigation" aria-label="Main navigation">
                    <div class="div-wrapper">
                        <a class="text-2" id="dashboardLink" aria-current="page">Dashboard</a>
                    </div>
                    <button class="text-2" id="menuLink" type="button" aria-label="Menu">Menu</button>
                    <button class="text-2" id="ordersLink" type="button" aria-label="Orders">Orders</button>
                    <button class="text-2" id="customersLink" type="button" aria-label="Customers">Customers</button>
                    <button class="text-2" id="productsLink" type="button" aria-label="Products">Prodcuts</button>
                    <button class="text-2" id="promosLink" type="button" aria-label="Promos">Promos</button>
                    <button class="text-2" id="settingsLink" type="button" aria-label="Settings">Settings</button>
                </nav>
            </div>
            <div class="frame-2">
                <button class="notification-button" type="button" aria-label="Notifications">
                    <img class="huge-icon-device" src="~/notification.svg" alt="" />
                </button>
                <div class="frame-3">
                    <img class="rectangle" src="~/Rectangle 3463281.png" alt="Jack Lowies profile picture" />
                    <div class="frame-4">
                        <div class="text-3">Jack Lowies</div>
                        <div class="text-4">Cashier</div>
                    </div>
                    <button class="dropdown-button" type="button" aria-label="User menu" aria-expanded="false">
                        <img class="huge-icon-arrows" src="~/direction-down 01.svg" alt="" />
                    </button>
                </div>
            </div>
        </header>
