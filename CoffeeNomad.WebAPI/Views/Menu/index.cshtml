                    <article class="CARD" role="region" aria-labelledby="product-title">
                        <div class="rectangle" aria-hidden="true"></div>
                        <section class="frame">
                            <div class="div">
                                <header class="frame-2">
                                    <h1 class="already-have-an" id="product-title">Matcha</h1>
                                    <span class="text-wrapper" aria-label="Price: $20">$20</span>
                                </header>
                                <div class="div">
                                    <label class="text-wrapper-2" for="cup-size-group">Cup Size</label>
                                    <fieldset class="QTY" id="cup-size-group" role="radiogroup" aria-labelledby="cup-size-label">
                                        <legend class="sr-only" id="cup-size-label">Select cup size</legend>
                                        <label class="delete">
                                            <input type="radio" name="cup-size" value="s" class="sr-only" aria-describedby="size-s-label" />
                                            <span class="text-wrapper-3" id="size-s-label">S</span>
                                        </label>
                                        <label class="delete">
                                            <input type="radio" name="cup-size" value="m" class="sr-only" aria-describedby="size-m-label" />
                                            <span class="text-wrapper-3" id="size-m-label">M</span>
                                        </label>
                                        <label class="delete">
                                            <input type="radio" name="cup-size" value="l" class="sr-only" aria-describedby="size-l-label" />
                                            <span class="text-wrapper-3" id="size-l-label">L</span>
                                        </label>
                                    </fieldset>
                                </div>
                            </div>
                            <div class="item-container-button">
                                <button class="item-button" type="button" aria-label="Add Matcha to shopping cart">
                                    <span class="button-2">Add to Cart</span>
                                    </span>
                                </button>
                            </div>
                        </section>
                        <img class="image"
                             src="~/image 1607.png"
                             alt="Matcha latte with green foam art in a brown ceramic cup and saucer" />
